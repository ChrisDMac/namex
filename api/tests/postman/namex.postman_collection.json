		{
			"name": "GetNext testing",
			"description": "",
			"item": [
				{
					"name": "requests/queues/@me/oldest getnext_test",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "f4c66699-a1b6-4384-a237-ba186c5b8037",
								"type": "text/javascript",
								"exec": [
									"pm.test(\"Response time is less than 200ms\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(200);",
									"});",
									"",
									"var jsonData = pm.response.json();",
									"if (typeof jsonData.nameRequest !== 'undefined' || jsonData.nameRequest !== null) {",
									"   pm.environment.set(\"queued_nr\", jsonData.nameRequest);",
									"}else{",
									"   pm.environment.unset(\"queued_nr\");    ",
									"}",
									"",
									"//tests what type of authorization the userid has",
									"var userid = pm.environment.get(\"userid\");",
									"function getnext_auth_test () {",
									"    if (userid == \"postman-examiner\" || userid == \"postman-manager\") {",
									"        return 'authorized';",
									"    } else if (userid == \"postman-support\") {",
									"        return 'forbidden';",
									"    } else {",
									"        return 'unauthorized';",
									"    }",
									"}",
									"",
									"//test for correct status code",
									"if (getnext_auth_test() == 'authorized') {",
									"    pm.test(\"Status code is 200\", function () {",
									"        pm.response.to.have.status(200);",
									"    });",
									"} else if (getnext_auth_test() == 'forbidden') {",
									"    pm.test(\"Status code is 403\", function () {",
									"        pm.response.to.have.status(403);",
									"    });",
									"} else {",
									"    pm.test(\"Status code is 401\", function () {",
									"        pm.response.to.have.status(401);    ",
									"    });",
									"}",
									"",
									"//test userid -- check if authorized to get next from queue",
									"if (getnext_auth_test() == 'authorized') {",
									"    pm.test(\"Authorized for GetNext\", function () {",
									"        if (userid !== \"postman-examiner\") {",
									"            pm.expect(userid).to.eql(\"postman-manager\");",
									"        }",
									"    });",
									"} else if (getnext_auth_test() == 'forbidden') {",
									"    pm.test(\"Not Authorized for GetNext\", function () {",
									"        pm.expect(userid).to.eql(\"postman-support\");",
									"    });",
									"    pm.test(\"Correct error message\", function () {",
									"        pm.expect(jsonData.message).to.eql(\"Error: You do not have access to the Name Request queue.\")",
									"    });",
									"} else {",
									"    pm.test(\"Unauthorized user\", function () {",
									"        pm.expect(jsonData.error).to.eql(\"invalid_token\");",
									"        pm.expect(jsonData.error_description).to.eql(\"Token required but invalid\");",
									"    });",
									"}",
									"//set user types in users -- add new ones to this array to test more",
									"//run GetNext testing folder from runner -- set iterations to length of users array to test all user types",
