                        "labels": {
                            "app": "nro-extractor-runtime",
                            "deploymentconfig": "nro-extractor-runtime"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "nro-extractor-runtime",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "protocol": "TCP"
                                    }
                                ],
                                "env": [
                                    {
                                        "name": "DATABASE_USERNAME",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-user"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_PASSWORD",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-password"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_NAME",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-name"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_HOST",
                                        "valueFrom": {
                                            "configMapKeyRef": {
                                                "name": "namex-api-config",
                                                "key": "DB_HOST"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_PORT",
                                        "value": "5432"
                                    },
                                    {
                                        "name": "FDW_DATABASE_PORT",
                                        "value": "5432"
                                    },
                                    {
                                        "name": "FDW_DATABASE_NAME",
                                        "value": "BC_REGISTRIES_NAMES"
                                    },
                                    {
                                        "name": "FDW_DATABASE_HOST",
                                        "value": "postgresql-oracle-names-fdw"
                                    },
                                    {
                                        "name": "FDW_DATABASE_USERNAME",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql-oracle-names-fdw",
                                                "key": "database-user"
                                            }
                                        }
                                    },
                                    {
                                        "name": "FDW_DATABASE_PASSWORD",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql-oracle-names-fdw",
                                                "key": "database-password"
                                            }
                                        }
                                    }
                                ],
                                "resources": {},
                                "terminationMessagePath": "/dev/termination-log",
                                "terminationMessagePolicy": "File",
                                "imagePullPolicy": "Always"
                            }
                        ],
                        "restartPolicy": "Always",
                        "terminationGracePeriodSeconds": 30,
                        "dnsPolicy": "ClusterFirst",
                        "securityContext": {},
                        "schedulerName": "default-scheduler"
