                "labels": {
                    "app": "nro-extractor-runtime"
                }
            },
            "spec": {
                "strategy": {
                    "type": "Rolling",
                    "rollingParams": {
                        "updatePeriodSeconds": 1,
                        "intervalSeconds": 1,
                        "timeoutSeconds": 600,
                        "maxUnavailable": "25%",
                        "maxSurge": "25%"
                    },
                    "resources": {},
                    "activeDeadlineSeconds": 21600
                },
                "triggers": [
                    {
                        "type": "ImageChange",
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "namex-feeder"
                            ],
                            "from": {
                                "kind": "ImageStreamTag",
                                "namespace": "servicebc-ne-tools",
                                "name": "nro-extractor-runtime:latest"
                            }
                        }
                    },
                    {
                        "type": "ConfigChange"
                    }
                ],
                "replicas": 1,
                "test": false,
                "selector": {
                    "app": "nro-extractor-runtime",
                    "deploymentconfig": "nro-extractor-runtime"
